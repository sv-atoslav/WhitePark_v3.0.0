<p id="notice"><%= notice %></p>
<%= link_to 'Edit', action: "edit"  %> |
<%= link_to 'Back', action: "index" %> |
<a href="/events/<%= @event.eng_title %>">Beauty</a>
<p><strong>Category:</strong><%= @event.category %></p>
<p><strong>Title:</strong><%=    @event.ru_title %></p>
<p><strong>Description:</strong><%= @event.description.html_safe %></p>
<p><strong>Icon:</strong><%= image_tag Photo.try_get_way_to_img_tag_by_id(@event.icon), height: "100px", width:"100px" %></p>
<p><strong>Many fotos:</strong></p><% @old_list.each do |one_foto_id| %>
	<%= image_tag Photo.try_get_way_to_img_tag_by_id(one_foto_id) , height: "100px" %>
<% end %>.