<div align = "center">
	<h2><%= @event.ru_title %></h2>
	<hr class="spec-hr">
	<%= @event.description.html_safe %>
	<div class="about-us-image-set"><% @old_list.each do |one_photo_id| %>
		<% way = Photo.try_get_way_to_img_tag_by_id(one_photo_id) %>
        <% if way.nil? %><%= $INE.html_safe %>
        <% else %><%= image_tag(way, style: "height: 100px;") %>
        <% end  %>
	<% end %></div>
	<br>
</div>
