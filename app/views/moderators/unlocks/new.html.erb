<h2>Отправить инструкции по разблокировке</h2>
<%= form_for(resource, as: resource_name, url: unlock_path(resource_name), html: { method: :post }) do |f| %>
  <%= devise_error_messages! %>
  <div class="field">
    <%= f.label :email %><br />
    <%= f.email_field :email, autofocus: true %>
  </div>
  <div class="actions"><%= f.submit "Отправить инструкции по разблокировке" %></div>
<% end %>
<%= render "moderators/shared/links" %>
